<?xml version="1.0"?>
<dolfin xmlns:dolfin="http://fenicsproject.org">
  <parameters name="muflon-parameters">
    <parameters name="discretization">
      <parameter key="N" type="int" value="2" />
      <parameter key="PTL" type="int" value="1" />
    </parameters>
    <parameters name="model">
      <!-- eps spreads over 2 elements of the coarsest mesh -->
      <parameter key="eps" type="double" value="0.125"/>
      <parameters name="mobility">
        <parameter key="M0" type="double" value="3e-1"/>
        <parameter key="m"  type="int" value="2" />
        <!--parameter key="M0" type="double" value="8e-3"/-->
        <!--parameter key="m"  type="int" value="0" /-->
      </parameters>
      <parameters name="nu">
        <parameter key="1" type="double" value="10.0" />
        <parameter key="2" type="double" value="1.0" />
      </parameters>
      <parameters name="rho">
        <parameter key="1" type="double" value="1000.0" />
        <parameter key="2" type="double" value="100.0" />
      </parameters>
      <parameters name="sigma">
        <parameter key="12" type="double" value="24.5" />
      </parameters>
    </parameters>
  </parameters>
</dolfin>
